journal = {Biochimica et Biophysica Acta - Gene Regulatory Mechanisms},
keywords = {MRNA quality control,MRNA turnover,Nonsense-mediated mRNA decay,Translation termination},
month = {feb},
number = {6-7},
pages = {612--623},
pmid = {23435113},
title = {{Nonsense-mediated mRNA decay - Mechanisms of substrate mRNA recognition and degradation in mammalian cells}},
