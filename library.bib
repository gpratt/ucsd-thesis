@article{Hasegawa2009,
abstract = {Structure comparison opens a window into the distant past of protein evolution, which has been unreachable by sequence comparison alone. With 55{\&}{\#}xa0;000 entries in the Protein Data Bank and about 500 new structures added each week, automated processing, comparison, and classification are necessary. A variety of methods use different representations, scoring functions, and optimization algorithms, and they generate contradictory results even for moderately distant structures. Sequence mutations, insertions, and deletions are accommodated by plastic deformations of the common core, retaining the precise geometry of the active site, and peripheral regions may refold completely. Therefore structure comparison methods that allow for flexibility and plasticity generate the most biologically meaningful alignments. Active research directions include both the search for fold invariant features and the modeling of structural transitions in evolution. Advances have been made in algorithmic robustness, multiple alignment, and speeding up database searches.},
author = {Hasegawa, Hitomi and Holm, Liisa},
doi = {10.1016/j.sbi.2009.04.003},
file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Hasegawa, Holm - 2009 - Advances and pitfalls of protein structural alignment.pdf:pdf;:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Hasegawa, Holm - 2009 - Advances and pitfalls of protein structural alignment.html:html},
issn = {1879-033X},
journal = {Current Opinion in Structural Biology},
keywords = {Databases- Protein,Models- Molecular,Proteins,Structural Homology- Protein},
mendeley-tags = {Databases- Protein,Models- Molecular,Proteins,Structural Homology- Protein},
month = {jun},
number = {3},
pages = {341--348},
title = {{Advances and pitfalls of protein structural alignment}},
url = {http://www.sciencedirect.com/science/article/pii/S0959440X09000621 http://www.sciencedirect.com/science?{\_}ob=MiamiImageURL{\&}{\_}cid=272019{\&}{\_}user=4429{\&}{\_}pii=S0959440X09000621{\&}{\_}check=y{\&}{\_}origin={\&}{\_}coverDate=30-Jun-2009{\&}view=c{\&}wchp=dGLbVlV-zSkzV{\&}md5=55bd967058178c0b},
