file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Rossbach et al. - 2014 - Crosslinking-immunoprecipitation (iCLIP) analysis reveals global regulatory roles of hnRNP L.pdf:pdf},
isbn = {1555-8584 (Electronic)$\backslash$r1547-6286 (Linking)},
issn = {1547-6286},
