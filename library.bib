keywords = {Amino Acid Sequence,Caspase 3,Caspase 3: metabolism,Data Collection,Escherichia coli Proteins,Escherichia coli Proteins: chemistry,Escherichia coli Proteins: genetics,Hydrolysis,Lysine,Lysine: chemistry,Molecular Sequence Data,Peptide Hydrolases,Peptide Hydrolases: metabolism,Peptides,Peptides: chemistry,Proteins,Proteins: chemistry,Proteomics,Proteomics: methods,Recombinant Proteins,Recombinant Proteins: chemistry,Recombinant Proteins: genetics,Spectrometry, Mass, Matrix-Assisted Laser Desorpti,Substrate Specificity},
mendeley-tags = {Data Collection},
