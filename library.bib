pmid = {27018577},
publisher = {NIH Public Access},
title = {{Robust transcriptome-wide discovery of RNA-binding protein binding sites with enhanced CLIP (eCLIP)}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27018577 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4887338},
volume = {13},
