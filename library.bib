@article{Townley-Tilson2006,
abstract = {The replication-dependent histone mRNAs are cell-cycle-regulated and expressed only during S phase. In contrast to all other eukaryotic mRNAs, the histone mRNAs end in a highly conserved 16-nucleotide stem-loop rather than a poly(A) tail. The stem-loop is necessary and sufficient for the post-transcriptional regulation of histone mRNA during the cell cycle. The histone mRNA 3' stem-loop is bound by the stem-loop binding protein (SLBP) that is involved in pre-mRNA processing, translation, and stability of histone mRNA. Immunoprecipitation (IP) of RNA-binding proteins (RBPs) followed by microarray analysis has been used to identify the targets of RNA-binding proteins. This method is sometimes referred to as RIP-Chip (RNA IP followed by microarray analysis). Here we introduce a variation on the RIP-Chip method that uses a recombinant RBP to identify mRNA targets in a pool of total RNA; we call this method recombinant, or rRIP-Chip. Using this method, we show that recombinant SLBP binds exclusively to all five classes of histone mRNA. We also analyze the messages bound to the endogenous SLBP on polyribosomes by immunoprecipitation. We use two different microarray platforms to identify enriched mRNAs. Both platforms demonstrate remarkable specificity and consistency of results. Our data suggest that the replication-dependent histone mRNAs are likely to be the sole target of SLBP.},
author = {Townley-Tilson, W H Davin and Pendergrass, Sarah A and Marzluff, William F and Whitfield, Michael L},
doi = {10.1261/rna.76006},
isbn = {1355-8382 (Print)$\backslash$n1355-8382 (Linking)},
issn = {1355-8382},
journal = {RNA},
keywords = {Base Sequence,Gene Expression,Genome,HeLa Cells,Histones,Histones: genetics,Histones: metabolism,Human,Humans,Immunoprecipitation,In Vitro Techniques,Macromolecular Substances,Messenger,Messenger: genetics,Messenger: metabolism,Nuclear Proteins,Nuclear Proteins: genetics,Nuclear Proteins: isolation {\&} purification,Nuclear Proteins: metabolism,Oligonucleotide Array Sequence Analysis,Polyribosomes,Polyribosomes: metabolism,Protein Binding,RNA,RNA-Binding Proteins,RNA-Binding Proteins: genetics,RNA-Binding Proteins: isolation {\&} purification,RNA-Binding Proteins: metabolism,Recombinant Proteins,Recombinant Proteins: genetics,Recombinant Proteins: metabolism,Ribonucleoproteins,Ribonucleoproteins: chemistry,Ribonucleoproteins: genetics,Ribonucleoproteins: metabolism,mRNA Cleavage and Polyadenylation Factors,mRNA Cleavage and Polyadenylation Factors: genetic,mRNA Cleavage and Polyadenylation Factors: isolati,mRNA Cleavage and Polyadenylation Factors: metabol},
