editor = {Abdelhay, Eliana Saul Furquim Werneck},
file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Golob et al. - 2011 - Evidence That Gene Activation and Silencing during Stem Cell Differentiation Requires a Transcriptionally Paused I.pdf:pdf},
issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Animals,Cell Differentiation,Cell Differentiation: genetics,Cell Line,Embryonic Stem Cells,Embryonic Stem Cells: cytology,Embryonic Stem Cells: metabolism,Gene Expression Regulation, Developmental,Gene Silencing,Genetic Loci,Genetic Loci: genetics,Genome, Human,Genome, Human: genetics,Humans,Mice,Models, Genetic,Open Reading Frames,Open Reading Frames: genetics,Transcription, Genetic,Transcriptional Activation,Transcriptional Activation: genetics},
