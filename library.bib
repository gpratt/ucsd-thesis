file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Van Nostrand et al. - 2016 - Robust transcriptome-wide discovery of RNA-binding protein binding sites with enhanced CLIP (eCLIP).pdf:pdf},
isbn = {1548-7105 (Electronic)$\backslash$r1548-7091 (Linking)},
issn = {15487105},
journal = {Nature Methods},
