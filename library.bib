eprint = {arXiv:1507.02142v2},
file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Jung et al. - 2014 - Impact of sequencing depth in ChIP-seq experiments.pdf:pdf},
isbn = {1362-4962 (Electronic)$\backslash$r0305-1048 (Linking)},
