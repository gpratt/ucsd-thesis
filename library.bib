number = {2},
pages = {331--338},
pmid = {22771914},
publisher = {Elsevier B.V.},
title = {{Domains involved in TAF15 subcellular localisation: Dependence on cell type and ongoing transcription}},
url = {http://dx.doi.org/10.1016/j.gene.2012.06.088},
volume = {506},
year = {2012}
