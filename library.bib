keywords = {Chromatography, High Pressure Liquid,Methanococcus,Methanococcus: metabolism,Porphyromonas gingivalis,Porphyromonas gingivalis: metabolism,Prokaryotic Cells,Proteins,Proteins: chemistry,Tandem Mass Spectrometry,msms},
