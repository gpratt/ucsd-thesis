pages = {545--54},
pmid = {21595825},
title = {{Molecular profiling indicates orthotopic xenograft of glioma cell lines simulate a subclass of human glioblastoma.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21595825},
volume = {16},
year = {2012}
}
