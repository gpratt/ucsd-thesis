file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Nellore et al. - 2016 - Human splicing diversity and the extent of unannotated splice junctions across human RNA-seq samples on the Sequ.pdf:pdf},
issn = {1474760X},
journal = {Genome Biology},
keywords = {Intron,RNA-seq,Splicing},
month = {dec},
