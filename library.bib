pages = {240--251},
pmid = {23810193},
publisher = {Elsevier Inc.},
title = {{Ribosome profiling provides evidence that large noncoding RNAs do not encode proteins}},
url = {http://dx.doi.org/10.1016/j.cell.2013.06.009},
volume = {154},
year = {2013}
}
