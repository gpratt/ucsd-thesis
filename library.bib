number = {Database issue},
pages = {D1144--9},
pmid = {22086960},
title = {{PrimerBank: a PCR primer database for quantitative gene expression analysis, 2012 update.}},
volume = {40},
year = {2012}
}
@article{Guo2010a,
abstract = {MicroRNAs (miRNAs) are endogenous approximately 22-nucleotide RNAs that mediate important gene-regulatory events by pairing to the mRNAs of protein-coding genes to direct their repression. Repression of these regulatory targets leads to decreased translational efficiency and/or decreased mRNA levels, but the relative contributions of these two outcomes have been largely unknown, particularly for endogenous targets expressed at low-to-moderate levels. Here, we use ribosome profiling to measure the overall effects on protein production and compare these to simultaneously measured effects on mRNA levels. For both ectopic and endogenous miRNA regulatory interactions, lowered mRNA levels account for most ({\textgreater}/=84{\%}) of the decreased protein production. These results show that changes in mRNA levels closely reflect the impact of miRNAs on gene expression and indicate that destabilization of target mRNAs is the predominant reason for reduced protein output.},
author = {Guo, Huili and Ingolia, Nicholas T and Weissman, Jonathan S and Bartel, David P},
doi = {10.1038/nature09267},
