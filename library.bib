journal = {Genes {\&} development},
keywords = {Animals,Cell Differentiation,DNA-Directed RNA Polymerases,DNA-Directed RNA Polymerases: metabolism,Data Collection,Embryonic Stem Cells,Embryonic Stem Cells: cytology,Embryonic Stem Cells: enzymology,Fibroblasts,Fibroblasts: cytology,Fibroblasts: enzymology,Gene Expression Regulation, Enzymologic,Genome-Wide Association Study,Histones,Histones: metabolism,Mice,Promoter Regions, Genetic},
mendeley-tags = {Data Collection},
month = {apr},
