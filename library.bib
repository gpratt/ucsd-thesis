file = {:Users/gpratt/Library/Application Support/Mendeley Desktop/Downloaded/Keene, Komisarow, Friedersdorf - 2006 - RIP-Chip the isolation and identification of mRNAs, microRNAs and protein components of ribonucl.pdf:pdf},
