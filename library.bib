title = {{Human mitochondrial RNA decay mediated by PNPaseâ€“hSuv3 complex takes place in distinct foci}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23221631 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3553951 https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gks1130},
