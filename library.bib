journal = {Neuropathology},
keywords = {Amyotrophic lateral sclerosis,Insulin-like growth factor I,Insulin-like growth factor II,Leukemia inhibitory factor antibody,Microglia},
number = {3},
pages = {257--268},
pmid = {17645240},
title = {{Expression of insulin-like growth factor-II and leukemia inhibitory factor antibody immunostaining on the ionized calcium-binding adaptor molecule 1-positive microglias in the spinal cord of amyotrophic lateral sclerosis patients}},
