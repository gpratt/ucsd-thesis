journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Animals,Cell Line,Chromatin Immunoprecipitation,DNA,Erythrocytes,Erythrocytes: metabolism,Humans,Mice,Regulatory Elements,Sequence Analysis,Transcription Factors,Transcription Factors: metabolism,Transcriptional},
