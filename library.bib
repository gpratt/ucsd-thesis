journal = {Nature},
keywords = {Base Sequence,Genetic Techniques,Genome-Wide Association Study,Molecular Sequence Data,Nucleic Acid Conformation,RNA, Fungal,RNA, Fungal: chemistry,RNA, Messenger,RNA, Messenger: chemistry,Saccharomyces cerevisiae,Saccharomyces cerevisiae: chemistry,Saccharomyces cerevisiae: genetics,Transcription, Genetic},
month = {sep},
number = {7311},
pages = {103--7},
pmid = {20811459},
