@article{Tlusty2010a,
abstract = {The genetic code maps the sixty-four nucleotide triplets (codons) to twenty amino-acids. While the biochemical details of this code were unraveled long ago, its origin is still obscure. We review information-theoretic approaches to the problem of the code's origin and discuss the results of a recent work that treats the code in terms of an evolving, error-prone information channel. Our model - which utilizes the rate-distortion theory of noisy communication channels - suggests that the genetic code originated as a result of the interplay of the three conflicting evolutionary forces: the needs for diverse amino-acids, for error-tolerance and for minimal cost of resources. The description of the code as an information channel allows us to mathematically identify the fitness of the code and locate its emergence at a second-order phase transition when the mapping of codons to amino-acids becomes nonrandom. The noise in the channel brings about an error-graph, in which edges connect codons that are likely to be confused. The emergence of the code is governed by the topology of the error-graph, which determines the lowest modes of the graph-Laplacian and is related to the map coloring problem.},
author = {Tlusty, Tsvi},
doi = {10.1016/j.plrev.2010.06.002},
issn = {1873-1457},
journal = {Physics of life reviews},
keywords = {Amino Acids,Amino Acids: chemistry,Amino Acids: genetics,Amino Acids: metabolism,Biogenesis,Evolution, Molecular,Genetic Code,Genetic Code: genetics,Humans,Information Theory,Information Theroy,Models, Statistical,fun},
mendeley-tags = {Information Theroy,fun},
month = {sep},
number = {3},
pages = {362--76},
pmid = {20558115},
title = {{A colorful origin for the genetic code: information theory, statistical mechanics and the emergence of molecular codes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20558115},
volume = {7},
year = {2010}
}
@article{Katsuno2011,
abstract = {Transforming growth factor $\beta$ (TGF-$\beta$), a pleiotropic cytokine, regulates a diverse range of cellular responses, such as proliferation, differentiation, migration, and apoptosis. The TGF-$\beta$1, -$\beta$2, and -$\beta$3 isoforms are expressed by neurons and glial cells, and their receptors are expressed throughout the central nervous system. Several lines of evidence demonstrate that TGF-$\beta$ signaling protects neurons from glutamate-mediated excitotoxicity, a putative mechanism underlying the pathogenesis of various neurodegenerative disorders such as amyotrophic lateral sclerosis (ALS). Recent studies indicate that the TGF-$\beta$-Smad2/3 pathway restores motor function in a mouse model of ALS, and that disruption of TGF-$\beta$ signaling due to the transcriptional dysregulation of its receptor is associated with polyglutamine-induced motor neuron damage in spinal and bulbar muscular atrophy. Moreover, the TGF-$\beta$-Smad2/3 pathway regulates the function of glial cells, although the implication of this regulation in neurodegeneration remains elusive. Conversely, myostatin, a member of the TGF-$\beta$ superfamily, has gained attention as a potential therapeutic target for neuromuscular disorders because genetic deletion of this factor results in increased muscle volume. Signal transduction by BMP, a member of the TGF-$\beta$ super family, regulates the function and growth of the neuromuscular junction, while the disruption of this signaling has been reported in animal models of hereditary spastic paraplegia. These findings support the hypothesis that the disruption of TGF-$\beta$ signaling is an important molecular event in the pathogenesis of motor neuron diseases, and that the modification of this signaling pathway represents a new therapeutic strategy against these devastating disorders.},
author = {Katsuno, M and Adachi, H and Banno, H and Suzuki, K and Tanaka, F and Sobue, G},
doi = {10.2174/156652411794474356},
file = {:Users/gpratt/Downloads/papers/EECE6C89542B410E1439856315A0C3FA92A1D3C7A9.pdf:pdf},
isbn = {8152744239},
issn = {15665240},
journal = {Current molecular medicine},
keywords = {amyotrophic lateral,atrophy,axonal,hereditary spastic paraplegia,motor neuron,neuromuscular junction,sclerosis,spinal and bulbar muscular,spinal muscular atrophy,tgf-,transforming growth factor},
number = {1},
pages = {48--56},
pmid = {21189118},
title = {{Transforming growth factor-$\beta$ signaling in motor neuron diseases.}},
volume = {11},
year = {2011}
}
@article{Khorshid2011,
abstract = {The stability, localization and translation rate of mRNAs are regulated by a multitude of RNA-binding proteins (RBPs) that find their targets directly or with the help of guide RNAs. Among the experimental methods for mapping RBP binding sites, cross-linking and immunoprecipitation (CLIP) coupled with deep sequencing provides transcriptome-wide coverage as well as high resolution. However, partly due to their vast volume, the data that were so far generated in CLIP experiments have not been put in a form that enables fast and interactive exploration of binding sites. To address this need, we have developed the CLIPZ database and analysis environment. Binding site data for RBPs such as Argonaute 1-4, Insulin-like growth factor II mRNA-binding protein 1-3, TNRC6 proteins A-C, Pumilio 2, Quaking and Polypyrimidine tract binding protein can be visualized at the level of the genome and of individual transcripts. Individual users can upload their own sequence data sets while being able to limit the access to these data to specific users, and analyses of the public and private data sets can be performed interactively. CLIPZ, available at http://www.clipz.unibas.ch, aims to provide an open access repository of information for post-transcriptional regulatory elements.},
