pages = {1109--21},
pmid = {21536720},
title = {{Prioritizing candidate disease genes by network-based boosting of genome-wide association data.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3129253{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {21},
year = {2011}
}
