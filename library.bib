pages = {302--307},
pmid = {17406249},
publisher = {Nature Publishing Group},
title = {{RIP-Chip: The isolation and identification of mRNAs, microRNAs and protein components of ribonucleoprotein complexes from cell extracts}},
url = {http://www.nature.com/doifinder/10.1038/nprot.2006.47},
volume = {1},
